<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="TwoColorBall">
        <attr name="ball_color" format="string" />
        <attr name="ball_enable" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="BaseDoT">
        <attr name="circleColor" format="color|reference" />
    </declare-styleable>

    <declare-styleable name="BaseDoTView">
        <attr name="slideCount" format="integer|reference" />
        <attr name="selectPosition" format="integer|reference" />
        <attr name="selectedIndicatorColor" format="color|reference" />
        <attr name="unselectedIndicatorColor" format="color|reference" />
        <attr name="circleRadius" format="dimension|reference" />
    </declare-styleable>

    <declare-styleable name="EmojiRainLayout">
        <attr name="per" format="integer" />
        <attr name="duration" format="integer" />
        <attr name="dropDuration" format="integer" />
        <attr name="dropFrequency" format="integer" />
    </declare-styleable>

</resources>